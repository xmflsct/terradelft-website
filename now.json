{
  "build": {
    "env": {
      "CONTENTFUL_HOST": "@terradelft-website-contentful-host",
      "CONTENTFUL_KEY_GATSBY": "@terradelft-website-contentful-key-gatsby",
      "CONTENTFUL_SPACE": "@terradelft-website-contentful-space",
      "CONTENTFUL_ENVIRONMENT": "@terradelft-website-contentful-environment",
      "GATSBY_RECAPTCHA_PUBLIC_KEY": "@terradelft-website-recaptcha-public-key",
      "GATSBY_STRIPE_PUBLIC_KEY": "@terradelft-website-stripe-public-key"
    }
  },
  "env": {
    "CONTENTFUL_HOST": "@terradelft-website-contentful-host",
    "CONTENTFUL_KEY_CHECKOUT": "@terradelft-website-contentful-key-checkout",
    "CONTENTFUL_SPACE": "@terradelft-website-contentful-space",
    "CONTENTFUL_ENVIRONMENT": "@terradelft-website-contentful-environment",
    "RECAPTCHA_PRIVATE_KEY": "@terradelft-website-recaptcha-private-key",
    "STRIPE_PRIVATE_KEY": "@terradelft-website-stripe-private-key"
  },
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404" }
  ]
}
