{
  "name": "terradelft-website",
  "description": "Terra Delft Website",
  "version": "0.1.0",
  "author": "xmflsct <me@xmflsct.com>",
  "private": true,
  "license": "GPL-3.0-or-later",
  "sideEffects": false,
  "scripts": {
    "build": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css && remix build --sourcemap && sentry-upload-sourcemaps",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --kv TERRADELFT_WEBSITE --binding $(cat .env)",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^16.0.2",
    "@contentful/rich-text-react-renderer": "^15.16.2",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.7",
    "@remix-run/cloudflare": "^1.9.0",
    "@remix-run/cloudflare-pages": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@sentry/remix": "^7.27.0",
    "@stripe/stripe-js": "^1.46.0",
    "get-youtube-id": "^1.0.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "i18n-iso-countries": "^7.5.0",
    "i18next": "^22.4.5",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-medium-image-zoom": "^5.1.2",
    "react-select": "^5.7.0",
    "remix-i18next": "^5.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@contentful/rich-text-types": "^16.0.2",
