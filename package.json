    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "got": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-import": "^15.0.0",
    "schema-dts": "^1.1.0",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
