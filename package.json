    "got": "^12.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-import": "^15.1.0",
    "schema-dts": "^1.1.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
