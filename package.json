    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-medium-image-zoom": "^5.2.4",
    "react-select": "^5.8.0",
    "remix-i18next": "^6.1.0"
