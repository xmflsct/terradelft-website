{
  "name": "terradelft-website",
  "description": "Terra Delft Website",
  "version": "0.1.0",
  "author": "xmflsct <me@xmflsct.com>",
  "private": true,
  "license": "GPL-3.0-or-later",
  "sideEffects": false,
  "scripts": {
    "build": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --binding $(cat .env)",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@contentful/rich-text-plain-text-renderer": "^15.12.1",
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@remix-run/cloudflare": "^1.6.5",
    "@remix-run/cloudflare-pages": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "i18n-iso-countries": "^7.5.0",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.1",
    "react-select": "^5.4.0",
    "remix-i18next": "^4.1.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.6.5",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "schema-dts": "^1.1.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
